{
  "name": "xml2jsoneditor",
  "displayName": "Xml To Json Editor",
  "description": "Extension to convert bi-directional XML to JSON Editor",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization","Linters"
  ],
  "activationEvents": [],
  "publisher": "vyuvalv",
  "repository": "https://github.com/vyuvalv/xml2jsonConverter",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xml2jsoneditor.xmlToJson",
        "title": "ðŸš€ View XML File as JSON",
        "category": "editor"
      },
      {
        "command": "xml2jsoneditor.viewJson",
        "title": "ðŸ“º View in JSON Editor",
        "category": "editor"
      },
      {
        "command": "xml2jsoneditor.openFile",
        "title": "ðŸŽ± Open File in JSON Editor",
        "category": "explorer"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "xml2jsoneditor.xmlToJson",
          "when": "resourceLangId == xml"
        },
        {
          "command": "xml2jsoneditor.viewJson",
          "when": "resourceLangId == json"
        }
      ],
      "explorer/context": [
        {
          "command": "xml2jsoneditor.openFile",
          "when": "resourceLangId == xml || resourceLangId == json"
        }
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "xml2js": "^0.6.0"
  }
}
